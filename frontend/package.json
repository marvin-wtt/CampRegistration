    "http-status": "^1.7.4",
    "jsep": "^1.3.9",
    "jspdf": "^2.5.1",
    "pinia": "^2.1.7",
    "quasar": "^2.16.6",
    "showdown": "^2.1.0",
    "survey-core": "^1.11.7",
    "survey-creator-core": "^1.11.7",
    "survey-creator-vue": "^1.11.7",
    "survey-vue3-ui": "^1.11.7",
    "vue": "^3.4.33",

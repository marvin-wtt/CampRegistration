{
  "name": "@camp-registration/api",
  "version": "0.0.1",
  "type": "module",
  "description": "An API for camp registrations",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build/ && rimraf ./node_modules/",
    "build": "prisma generate && tsc && tsc-alias && cp ./src/entry.cjs ./build/ && npm run build:views",
    "build:views": "mkdirp ./build/views/emails/ && mjml src/views/emails/** -o ./build/views/emails/",
    "migrate": "npm run db:migrate",
    "db:migrate": "prisma migrate deploy",
    "db:reset": "npx prisma db push --force-reset --accept-data-loss",
    "db:truncate": "tsx prisma/scripts/truncate.ts",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "test": "npm run test:int",
    "test:unit": "vitest -c ./vitest.config.unit.ts",
    "test:unit:ui": "vitest -c ./vitest.config.unit.ts --ui",
    "test:int": "vitest -c ./vitest.config.integration.ts",
    "test:int:ui": "./scripts/run-integration.sh --ui",
    "lint": "eslint src",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,ts,md,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,ts,md,json}\" --ignore-path .gitignore",
    "dev": "npm run dev:views & npm run dev:serve",
    "dev:views": "mjml --watch src/views/emails/* -o ./src/views/emails/ ",
    "dev:serve": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "production": "NODE_ENV=production node build/entry.cjs"
  },
  "prisma": {
    "seed": "tsx prisma/seeders/index.ts"
  },
  "keywords": [],
  "author": "marvin-wtt <31454580+marvin-wtt@users.noreply.github.com>",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marvin-wtt/CampRegistration",
    "directory": "backend"
  },
  "imports": {
    "#app/*": "./src/app/*.js",
    "#assets/*": "./src/assets/*.js",
    "#config/*": "./src/config/*.js",
    "#core/*": "./src/core/*.js",
    "#guards/*": "./src/guards/*.js",
    "#i18n/*": "./src/i18n/*.js",
    "#jobs/*": "./src/jobs/*.js",
    "#middlewares/*": "./src/middlewares/*.js",
    "#routes/*": "./src/routes/*.js",
    "#utils/*": "./src/utils/*.js",
    "#views/*": "./src/views/*.js",
    "#types/*": "./src/types/*.js"
  },
  "dependencies": {
    "@camp-registration/common": "*",
    "@camp-registration/web": "*",
    "@prisma/client": "^6.2.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "countries-list": "^3.0.6",
    "croner": "^8.0.0",
    "dotenv": "^16.1.3",
    "dotenv-cli": "^7.2.1",
    "eslint-plugin-security": "^2.1.0",
    "express": "^5.0.1",
    "express-handlebars": "^8.0.1",
    "express-rate-limit": "^7.0.1",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "http-status": "^1.6.2",
    "i18next": "^23.7.3",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.9",
    "nodemailer-express-handlebars": "^6.1.0",
    "otpauth": "^9.3.6",
    "passport": "^0.7.0",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "qs": "^6.11.2",
    "survey-core": "^1.12.14",
    "ulidx": "^2.0.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@jest/globals": "^29.5.0",
    "@types/async-lock": "^1.4.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.2",
    "@types/mjml": "^4.7.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.5",
    "@types/nodemailer": "^6.4.8",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/passport": "^1.0.12",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-jwt": "^4.0.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^9.0.0",
    "mjml": "^5.0.0-alpha.4",
    "mkdirp": "^3.0.1",
    "nodemailer-mock": "^2.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^6.2.1",
    "prisma-json-types-generator": "^3.0.4",
    "rimraf": "^5.0.5",
    "smtp-tester": "^2.1.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^3.0.5",
    "vitest-mock-extended": "^2.0.2"
  }
}
